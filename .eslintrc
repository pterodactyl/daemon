{
  "plugins": [
    "lodash"
  ],
  "extends": [
    "airbnb",
    "plugin:lodash/recommended"
  ],
  "rules": {
      "indent": [2, 4],
      "func-names": 0,
      "strict": 0,
      "quote-props": 0,
      "consistent-return": 0,
      "max-len": 0,
      "lodash/prefer-map": 0,
      "arrow-parens": [2, "as-needed"]
  }
}
