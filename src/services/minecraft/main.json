{
    "latest": {
        "tag": "^(latest)$",
        "symlink": "vanilla"
    },
    "vanilla": {
        "tag": "^(vanilla){1}(-[\\w\\d.-]+)?$",
        "startup": {
            "done": ")! For help, type ",
            "userInteraction": [
                "Go to eula.txt for more info."
            ]
        },
        "stop": "stop",
        "configs": {
            "server.properties": {
                "parser": "properties",
                "find": {
                    "server-ip": "0.0.0.0",
                    "enable-query": "true",
                    "server-port": "{{ build.default.port }}",
                    "query.port": "{{ build.default.port }}"
                }
            },
            "spigot.yml": {
                "parser": "yaml",
                "find": {
                    "settings.restart-on-crash": "false"
                }
            }
        },
        "log": {
            "custom": false,
            "location": "logs/latest.log"
        },
        "query": "minecraft"
    },
    "spigot": {
        "tag": "^(spigot)$",
        "symlink": "vanilla"
    }
}
